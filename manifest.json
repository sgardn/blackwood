{
  "name": "Blackwood",
  "version": "1.0",
  "manifest_version" : 2,
  "description": "Scrape bridge data generated by BridgeComposer",
  "icons": { "48":"images/card48.png", "128":"images/card128.png" },  
  "content_scripts" : [
  {
	    "matches" : ["http://www.bridgespot.com/*"],
	    "js": [ "./scripts/jquery-1.6.2.min.js", "./scripts/inject.js"],
	    "run_at": "document_end"
  }
  ]
}